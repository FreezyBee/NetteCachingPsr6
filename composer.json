{
  "name": "freezy-bee/nette-caching-psr6",
  "type": "library",
  "description": "PSR-6 Bridge for Nette Caching",
  "keywords": [
    "nette",
    "freezy-bee",
    "psr-6",
    "cache"
  ],
  "homepage": "http://freezybee.ifire.cz",
  "license": "MIT",
  "authors": [
    {
      "name": "Jakub Janata",
      "email": "jakubjanata@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "nette/caching": "^3.0",
    "psr/cache": "^1.0 | ^2.0 | ^3.0"
  },
  "require-dev": {
    "nette/tester": "~2.0",
    "tracy/tracy": "^2.4",
    "phpstan/phpstan": "^1.8",
    "squizlabs/php_codesniffer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "FreezyBee\\NetteCachingPsr6\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FreezyBee\\NetteCachingPsr6\\Tests\\": "tests/"
    }
  }
}
